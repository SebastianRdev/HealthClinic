{
  "metadata": [
    {
      "src": [
        {
          "src": ".",
          "files": [ "HealthClinic.csproj" ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "docs/**.md", "toc.yml", "index.md" ]
      },
      {
        "files": [ "api/**.yml" ],
        "dest": "api"
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "overwrite": [ "docs/**.md" ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "HealthClinic Documentation",
      "_enableSearch": true
    },
    "template": [ "default" ]
  }
}
